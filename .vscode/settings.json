{
    "latex-workshop.latex.outDir": "%DIR%/out",
    "latex-workshop.formatting.latex": "latexindent",
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-shell-escape",
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "-outdir=%DIR%/out",
                "%DOC%"
            ],
            "env": {}
        }
    ],

    "cSpell.words": [
        "Annuitätendarlehen",
        "Ausb",
        "BEEG",
        "exkl",
        "Fachinformatikerausbildungsverordnung",
        "Kündigungsberechtigten",
        "Lieferer",
        "Mindest",
        "Rückwärtskalkulation",
        "Sonn",
        "Teilnahmeort",
        "Teilnutzenaspekten",
        "Tilgungsanteil",
        "Tilgungsbeträgen",
        "Tilgungsregelung",
        "Vorwärtskalkulation",
        "weiterleasen"
    ]
}
