{
  "latex-workshop.latex.outDir": "%DIR%/out",
  "latex-workshop.formatting.latex": "latexindent",
  "latex-workshop.latex.tools": [
    {
      "name": "latexmk",
      "command": "latexmk",
      "args": [
        "-shell-escape",
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-pdf",
        "-outdir=%DIR%/out",
        "%DOC%"
      ],
      "env": {}
    }
  ],

  "plantuml.exportIncludeFolderHeirarchy": false,
  "plantuml.exportOutDir": "figures",
  "plantuml.exportSubFolder": false,
  "plantuml.lintDiagramNoName": false,

  "editor.formatOnSave": false,

  "cSpell.words": [
    "Annuitätendarlehen",
    "Ausb",
    "BEEG",
    "Bitübertragung",
    "Bitübertragungsschicht",
    "enduml",
    "exkl",
    "Fachinformatikerausbildungsverordnung",
    "Kündigungsberechtigten",
    "Lieferer",
    "Mindest",
    "Rückwärtskalkulation",
    "softwarebasierte",
    "Sonn",
    "startuml",
    "Subnetting",
    "Teilnahmeort",
    "Teilnutzenaspekten",
    "textartig",
    "Tilgungsanteil",
    "Tilgungsbeträgen",
    "Tilgungsregelung",
    "Vorwärtskalkulation",
    "weiterleasen",
    "zustandsbehaftete"
  ]
}
